{
  "name": "shelljs-changelog",
  "version": "0.0.1",
  "description": "A simple script to create & update a changelog",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release:major": "node index.js major",
    "release:minor": "node index.js minor",
    "release:patch": "node index.js patch"
  },
  "bin": {
    "changelog": "index.js"
  },
  "keywords": [
    "changelog",
    "shelljs",
    "script",
    "update"
  ],
  "author": "Nate Fischer <ntfschr@gmail.com> (https://github.com/nfischer)",
  "license": "MIT",
  "dependencies": {
    "shelljs": "^0.7.0"
  },
  "devDependencies": {
    "shelljs-release": "^0.1.5"
  }
}
